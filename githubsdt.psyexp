        <Param val="keyDuration = 10;&amp;#10;Key_pressed = false;&amp;#10;(Key_release === 0);&amp;#10;color_1 = [1, 1, 1];&amp;#10;color_2 = [1, 1, 1];&amp;#10;color_3 = [1, 1, 1];&amp;#10;color_4 = [1, 1, 1];&amp;#10;color_5 = [1, 1, 1];&amp;#10;&amp;#10;psychoJS.eventManager.clearEvents({&quot;eventType&quot;: &quot;keyboard&quot;});&amp;#10;" valType="extendedCode" updates="constant" name="End JS Routine"/>
